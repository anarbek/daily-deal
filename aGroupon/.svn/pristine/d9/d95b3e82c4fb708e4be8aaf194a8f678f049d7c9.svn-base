@{
    ViewBag.Title = "About Us";
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/mainSiteTemplate/style/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/mainSiteTemplate/style/reset.css")" rel="stylesheet" type="text/css" />
    <!--[if IE 8]>
<link rel="stylesheet" href="@Url.Content("~/mainSiteTemplate/style/ie8.css")" media="screen, projection" type="text/css" >
<![endif]-->
    <!--[if IE 9]>
<link rel="stylesheet" href="@Url.Content("~/mainSiteTemplate/style/ie9.css")" media="screen, projection" type="text/css" >
<![endif]-->
    <script src="@Url.Content("~/mainSiteTemplate/script/jquery/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <!--MENU-->
    <link rel="stylesheet" media="screen" href="@Url.Content("~/mainSiteTemplate/script/menu/superfish.css")" />
    <script src="@Url.Content("~/mainSiteTemplate/script/menu/superfish.js")"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {

            //MENU
            $("ul.sf-menu").superfish();



            //BRISANJE ZADNJE LINIJE U MENIJU
            jQuery('.sf-menu li:last').attr('style', 'background:none');
            jQuery('.nav-my-profile ul li:last').attr('style', 'background:none');


            // HOVER-IMAGES
            jQuery('.images a').hover(function () {
                jQuery('img', this).stop().animate({ opacity: 0.4 }, 500);
            }, function () {
                jQuery('img', this).stop().animate({ opacity: 1 }, 300);
            });

            jQuery('.images-small a').hover(function () {
                jQuery('img', this).stop().animate({ opacity: 0.4 }, 500);
            }, function () {
                jQuery('img', this).stop().animate({ opacity: 1 }, 300);
            });

        });
</script>
    @RenderSection("CustomJS", required: false)
</head>
<body>
    <div id="container">
        @Html.Partial("_MainHeader")
        <!--Content Here-->
        <div class="content">
            <div class="wrapper">
                <div class="content-background">
                    <div class="content-wripper">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!--Content End-->
        </div>
        @Html.Partial("_MainFooter")
    </div>
</body>
</html>

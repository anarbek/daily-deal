@model aGrouponProjectMain.Controllers.tUserModelCustom
<p>
    @Html.ActionLink("Create New", "Create", new { IDRole = Model.IDRole })
</p>
<table  cellpadding="0" cellspacing="0" border="0" class="stdtable stdtablequick">
<colgroup>
                        <col class="con1">
                        <col class="con1">
                        <col class="con0">
                        <col class="con1">
                        <col class="con0">
                    </colgroup>
                    <thead>
                        <tr>
                            <th class="head1">UserName</th>
                            <th class="head1">Approved</th>
                            <th class="head0">Role</th>
                            <th class="head1">City</th>
                            <th class="head0">Category</th>
                            <th class="head1">LastLoginDate</th>
                             <th class="head0">Date Added</th>
                            <th class="head1">&nbsp;</th>
                        </tr>
                    </thead>
    

@foreach (var item in Model.UserList) {
    <tr>
                            <td class="con1"> @Html.DisplayFor(modelItem => item.UserName)</td>
                            <td class="center"> @Html.DisplayFor(modelItem => item.Approved)</td>
                            <td class="center"> @Html.DisplayFor(modelItem => item.tRole.Name)</td>
                            <td class="center"> @Html.DisplayFor(modelItem => item.tCategory.Name)</td>
                            <td class="center"> @Html.DisplayFor(modelItem => item.tCategory1.Name)</td>
                            <td class="center"> @Html.DisplayFor(modelItem => item.LastLoginDate)</td>
                            <td class="center"> @Html.DisplayFor(modelItem => item.DateAdded)</td>
                            <td class="center">@Html.ActionLink("Edit", "Edit", new { id = item.IDUser }, new { @class="toggle"}) |
                                                <a onclick='Delete("/User/DeleteUser",@item.IDUser)' href="#">Delete</a></td>
     </tr>
    
}

</table>
